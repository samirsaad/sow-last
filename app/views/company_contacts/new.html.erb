<h1>New contact</h1>

<% form_for(@company_contact) do |f| %>
  <%= f.error_messages %>
	<%= hidden_field_tag :company_id, params[:company_id] %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', '/companies/'+ (params[:company_id.to_s])%>